@model IEnumerable<OdeToFood.Data.Models.Restaurant>

@{
    ViewBag.Title = "Home Page";
}

<div class="jumbotron">
    <h1>ASP.NET</h1>
    <div>There are @Model.Count() restaurants on the db.</div>
</div>

<ul>
    @foreach (var restautant in Model)
    {
        <li>@restautant.Name</li>
    }
</ul>